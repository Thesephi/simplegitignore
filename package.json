{
  "name": "simplegitignore",
  "version": "0.1.0",
  "description": "generate your boilerplate gitignore (i.e. `npx simplegitignore`)",
  "scripts": {
    "start": "./node_modules/ts-node/dist/bin.js src/main.ts",
    "build": "./node_modules/@babel/cli/bin/babel.js -d bin --extensions \".ts\" src/main.ts",
    "prepare": "npm run build"
  },
  "main": "bin/main.js",
  "bin": "bin/main.js",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@types/node": "^10.9.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3"
  },
  "author": "Khang Dinh (https://kha.ngdinh.com/about-me)",
  "license": "MIT"
}
